{
    "family": "cicd_test_task",
    "taskRoleArn": "GitHub",
    "executionRoleArn": "GitHub",
    "networkMode": "awsvpc",
    "cpu": 256,
    "memory": 1,
    "containerDefinitions": [
        {
            "name": "cicd_test_container",
            "image": "https://hub.docker.com/_/ruby:2.5",
            "links": [
            ],
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [
            ],
            "command": [
                "rails server"
            ],
            "environment": [
            ],
            "mountPoints": [
            ],
            "volumesFrom": [
            ],
            "linuxParameters": {
                "initProcessEnabled": true
            },
            "secrets": [
            ],
            "user": "luke",
            "workingDirectory": "/etc/cicd_test",
            "privileged": false,
            "readonlyRootFilesystem": true,
            "dnsServers": [
            ],
            "dnsSearchDomains": [
            ],
            "extraHosts": [
            ],
            "dockerSecurityOptions": [
            ],
            "interactive": true,
            "pseudoTerminal": true,
            "dockerLabels": {
            },
            "ulimits": [
                {
                    "name": "nice",
                    "softLimit": 500,
                    "hardLimit": 1000
                }
            ]
        }
    ],
    "volumes": [
        {
            "name": "cicd_test_volume",
            "dockerVolumeConfiguration": {
                "scope": "task",
                "autoprovision": true
            }
        }
    ],
    "placementConstraints": [
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "pidMode": "task",
    "ipcMode": "host"
}
